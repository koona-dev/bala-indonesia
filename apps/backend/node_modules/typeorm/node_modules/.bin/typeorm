#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/HP/Documents/MANG_WID/development/web development/projects/bala-indonesia/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_reflect-metadata@0.2.2/node_modules/typeorm/node_modules:/mnt/c/Users/HP/Documents/MANG_WID/development/web development/projects/bala-indonesia/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_reflect-metadata@0.2.2/node_modules:/mnt/c/Users/HP/Documents/MANG_WID/development/web development/projects/bala-indonesia/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/HP/Documents/MANG_WID/development/web development/projects/bala-indonesia/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_reflect-metadata@0.2.2/node_modules/typeorm/node_modules:/mnt/c/Users/HP/Documents/MANG_WID/development/web development/projects/bala-indonesia/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_reflect-metadata@0.2.2/node_modules:/mnt/c/Users/HP/Documents/MANG_WID/development/web development/projects/bala-indonesia/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
