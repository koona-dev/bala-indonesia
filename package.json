{
  "name": "bala-indonesia",
  "version": "1.0.0",
  "description": "Making ecommerce website",
  "author": "Mang Wid",
  "type": "module",
  "license": "ISC",
  "packageManager": "pnpm@10.15.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:backend": "pnpm --filter backend dev",
    "dev:admin": "pnpm --filter admin dev",
    "dev:sales": "pnpm --filter sales dev",
    "start": "pnpm -r --parallel start",
    "dev": "pnpm -r --parallel dev",
    "test": "pnpm -r test",
    "clean": "pnpm -r --parallel clean && rimraf node_modules pnpm-lock.json"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "jest": "^30.0.2",
    "nodemon": "^3.1.10"
  }
}
